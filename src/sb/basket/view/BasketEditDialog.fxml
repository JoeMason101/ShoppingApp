<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="167.0" prefWidth="240.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sb.basket.view.BasketEditDialogController">
   <children>
      <GridPane layoutX="20.0" layoutY="14.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label styleClass="label-bright" text="Name" textOverrun="CLIP" />
            <Label styleClass="label-bright" text="Quantity" GridPane.rowIndex="1" />
            <Label styleClass="label-bright" text="Price" GridPane.rowIndex="2" />
            <TextField fx:id="nameField" GridPane.columnIndex="1" />
            <TextField fx:id="quantityField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="priceField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <ButtonBar layoutX="20.0" layoutY="113.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleOk" text="OK" />
            <Button minWidth="72.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="33.0" text="Cancel" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
